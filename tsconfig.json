{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "images/"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*"
  ]
}